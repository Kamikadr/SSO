{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "TokenGenerationSettings": {
    "SecretKey": "my_secret_key_for_sso",
    "AccessTokenLifetimeInterval": "00:05:00",
    "RefreshTokenLifetimeInterval": "1.00:00:00"
  }
}
